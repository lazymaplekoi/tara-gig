{
    "builds": [
      {
        "src": "tara_gig/wsgi.py",
        "use": "@vercel/python"
      },
      {
        "src": "build.sh",
        "use": "@vercel/static-build",
        "config": {
            "distDir": "staticfiles"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "tara_gig/wsgi.py"
      },
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      }
    ],
    "outputDirectory": "staticfiles"
}
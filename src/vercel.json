{
    "builds": [
      {
        "src": "tara_gig/wsgi.py",
        "use": "@vercel/python",
        "config": {
            "maxLambdaSize": "15mb",
            "runtime": "python3.11"
        }
      },
      {
        "src": "build.sh",
        "use": "@vercel/static-build",
        "config": {
            "distDir": "public"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "tara_gig/wsgi.py"
      },
      {
        "src": "/static/(.*)",
        "dest": "public/static/$1"
      }
    ],
    "outputDirectory": "public"
}